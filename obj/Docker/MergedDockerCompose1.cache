name: dockercompose115338524390537365
services:
  catalog.api:
    build:
      context: D:\Study\MicroSerivces\eShopping
      dockerfile: Services/Catalog/Catalog.API/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: Catalog.API
      target: base
    container_name: catalog.api
    depends_on:
      catalogdb:
        condition: service_started
    entrypoint:
    - tail
    - -f
    - /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      DOTNET_USE_POLLING_FILE_WATCHER: "1"
      DatabaseSettings__BrandsCollection: Brands
      DatabaseSettings__ConnectionString: mongodb://catalogdb:27017
      DatabaseSettings__DatabaseName: CatalogDb
      DatabaseSettings__ProductsCollection: Products
      DatabaseSettings__TypesCollection: Types
      NUGET_FALLBACK_PACKAGES: ""
    image: catalogapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /.nuget/packages  "/app/bin/Debug/net6.0/Catalog.API.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      default: null
    ports:
    - mode: ingress
      target: 80
      published: "7000"
      protocol: tcp
    restart: unless-stopped
    tty: true
    volumes:
    - type: bind
      source: D:\Study\MicroSerivces\eShopping\Services\Catalog\Catalog.API
      target: /app
      bind:
        create_host_path: true
    - type: bind
      source: D:\Study\MicroSerivces\eShopping
      target: /src
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\Annaati\vsdbg\vs2017u5
      target: /remote_debugger
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\Annaati\.nuget\packages\
      target: /.nuget/packages
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\Annaati\AppData\Roaming/ASP.NET/Https
      target: /root/.aspnet/https
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\Annaati\AppData\Roaming/ASP.NET/Https
      target: /home/app/.aspnet/https
      read_only: true
      bind:
        create_host_path: true
    - type: bind
